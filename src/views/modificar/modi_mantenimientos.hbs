<div class="container p-6">
    <div class="row">
        <div class="col-md-6 mx-auto">
        {{#each id}}
            <div class="card"> 
                <div class="card-bady">
                    <form action="/mantenimientos/modificar/{{id}}" method="post">
                        <div class="form-group">
                            <label for=""> M O D I F I C A R</label>


                        </div>LOCALES
                        <div class="form-group">
                            <input class="form-control" type="date" name="locales" value="{{locales}}">

                        </div>ESTRUCTURA
                        <div class="form-group">
                            <input class="form-control" type="text" name="estructura" value="{{estructura}}">

                        </div>SERVICIOS
                        <div class="form-group">
                            <input class="form-control" type="text" name="servicios" value="{{servicios}}">
                         
                        </div>PARQUEADERO
                        <div class="form-group">
                            <input class="form-control" type="text" name="parqueadero" value="{{parqueadero}}">

                        </div>JARDIN
                        <div class="form-group">
                            <input class="form-control" type="text" name="jardin" value="{{jardin}}">
                         

                        </div>DESCRIPCION
                        <div class="form-group">
                            <input class="form-control" type="text" name="descripcion" value="{{descripcion}}">

                         
                        <div class="form-group">
                            <button  type="submit" class="btn btn-success btn-block" >G U A R D A R</button>

        {{/each }}

                    </form>
                </div>
            </div>
        </div>
    </div>
</div> 